<?xml encoding="UTF-8" version="1.0"?>
<actions>
<action>
	<icon>Terminal</icon>
	<name>Apri un terminale qui</name>
	<command>exo-open --working-directory %f --launch TerminalEmulator</command>
	<description>Esempio di azione personalizzata</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>filefind</icon>
	<name>Trova in questa cartella</name>
	<command>catfish --path=%f</command>
	<description>Cerca file all&apos;interno di questa cartella</description>
	<patterns>*</patterns>
	<directories/>
</action>
<action>
	<icon>gcr-key</icon>
	<name>Cancellazione Sicura</name>
	<command>shred -fuz %F</command>
	<description>Elimina in maniera sicura i documenti dal tuo computer</description>
	<patterns>*</patterns>
	<text-files/>
</action>
</actions>

